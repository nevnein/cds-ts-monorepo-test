{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:cds": "yarn --cwd packages/cds dev",
    "build:cds": "yarn --cwd packages/cds build",
    "start:cds": "yarn --cwd packages/cds start",
    "dev:ts": "yarn --cwd packages/ts dev",
    "build:ts": "yarn --cwd packages/ts build",
    "start:ts": "yarn --cwd packages/ts start"
  }
}
